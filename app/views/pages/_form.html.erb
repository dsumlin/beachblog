<%= error_messages_for(@page) %>


<table Summary="Page form fields">
		<tr>
		<th>Subject ID</th>
		<td><%= f.select(:subject_id, 1..@subject_count) %></td>
		</tr>
		<tr>
		<th>Name</th>
		<td><%= f.text_field(:name) %></td>
		</tr>
   		<tr>
			<th>Position</th>
			<td><%= f.select(:position, 1..@page_count) %></td>
		</tr>
		<tr>
			<th>Visible</th>
			<td><%= f.check_box(:visible) %></td>
		</tr>
	
</table>